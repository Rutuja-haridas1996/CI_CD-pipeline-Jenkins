pipeline {
    agent any

    stages {
        stage('Checkout...') {
            steps {
                echo 'Checkout to Documents folder..'
                sh 'cd /home/rutujaharidas/Documents' 
                sh 'ls -l'
                dir("folder") {
                        sh "pwd"
                    }
                sh 'ls -l'
            }
        }
        
        
        
        
        
       
        
        
    }
}
